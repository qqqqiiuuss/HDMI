<mujoco model="wood_board">
    <compiler angle="radian" coordinate="local" />
    
    <default>
        <geom contype="1" conaffinity="1" friction="0.8 0.8 0.8" />
    </default>

    <asset>
        <material name="white" rgba="1 1 1 1" />
    </asset>

    <worldbody>
        <body name="wood_board" pos="0 0 0">
            <freejoint name="wood_board_root"/>
            <geom type="box" size="0.8 0.3 0.005" pos="0.8 0 0.005" material="white" mass="2.0"/>
        </body>
    </worldbody>

    <sensor>
        <framepos name="wood_board_pos" objtype="xbody" objname="wood_board" />
        <framequat name="wood_board_quat" objtype="xbody" objname="wood_board" />
    </sensor>
</mujoco>