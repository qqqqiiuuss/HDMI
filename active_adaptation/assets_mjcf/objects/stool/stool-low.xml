<mujoco model="stool">
  <compiler angle="radian" />
  
  <default>
    <default class="stool_leg">
      <geom type="box" size="0.02 0.02 0.1"/>
    </default>
  </default>
  
  <asset>
    <material name="white_wood" rgba="1 1 1 1" />
  </asset>
  
  <worldbody>
   <body name="stool" pos="0.7 0.0 0.4">
      <inertial mass="2.0" pos="0.0 0 -0.2" diaginertia="0.0334 0.02167 0.04167"/>

      <freejoint name="stool_low_root" />
      <site name="stool_root" pos="0 0 0" size="0.05" type="sphere" rgba="0.5 0.5 0.5 1"/>
      <site name="left_hand_contact" pos="0 0.2 -0.05" size="0.02" type="sphere" rgba="1 0 0 1"/>
      <site name="right_hand_contact" pos="0 -0.2 -0.05" size="0.02" type="sphere" rgba="1 0 0 1"/>

      <geom name="seat" type="box" size="0.125 0.175 0.02" pos="0 0 0" material="white_wood"/>
      <!-- <geom type="box" size="0.15 0.15 0.05" pos="0 0 -0.05" material="white_wood"/> -->

      <geom name="leg1" class="stool_leg" pos="0.105 0.155 -0.1" material="white_wood"/>
      <geom name="leg2" class="stool_leg" pos="0.105 -0.155 -0.1" material="white_wood"/>
      <geom name="leg3" class="stool_leg" pos="-0.105 0.155 -0.1" material="white_wood"/>
      <geom name="leg4" class="stool_leg" pos="-0.105 -0.155 -0.1" material="white_wood"/>
    </body>
  </worldbody>

  <sensor>
    <framepos name="stool_pos" objtype="xbody" objname="stool"/>
    <framequat name="stool_quat" objtype="xbody" objname="stool"/>
  </sensor>
</mujoco>